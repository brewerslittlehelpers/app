<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BrewersHelper.Views.History">
  <Label Text="{Binding MainText}" VerticalOptions="Center" HorizontalOptions="Center" />
  <StackLayout>
    <Label Text="This is the history page" VerticalOptions="Center" HorizontalOptions="Center" />
    <Button FontSize="11" Text="{Binding CompareButtonLabel}" Command="{Binding CompareButtonCommand}"
            VerticalOptions="Center" HorizontalOptions="Center" />
  </StackLayout>
</ContentPage>